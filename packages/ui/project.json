{
  "name": "ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ui/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "command": "vite build --config packages/ui/vite.config.ts",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["api:build", "schemas:build"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint packages/ui/src/**/*.ts",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tags": []
}
