{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug CLI",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/packages/cli/dist/src/index.js",
      "preLaunchTask": "npm: build",
      "args": ["generate", "-p", "${workspaceFolder}/apps/angular-example", "-o", "${workspaceFolder}/debugowcs.yaml"],
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "sourceMaps": true,
      "console": "integratedTerminal"
    }
  ]
}
